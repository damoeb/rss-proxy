{
  "name": "@rss-proxy/playground",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run gen-build-info",
    "start": "ng serve --proxy-config proxy.conf.json --watch=true",
    "gen-build-info": "ts-node -O '{\"module\": \"commonjs\"}' build-info.ts",
    "prebuild": "npm run gen-build-info",
    "pretest": "npm run gen-build-info",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "testCi": "ng test --configuration=ci",
    "lint": "prettier -w .",
    "e2e": "ng e2e",
    "clean": "rm -rf dist"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "bulma": "^0.9.4",
    "lodash": "^4.17.21",
    "rxjs": "~7.5.5",
    "toastr": "^2.1.4",
    "tslib": "^2.4.0",
    "urijs": "^1.19.11",
    "zone.js": "~0.11.5"
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.0",
    "@angular/cli": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/compiler-cli": "~13.1.0",
    "@angular/language-service": "~13.1.0",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/lodash": "^4.14.182",
    "@types/toastr": "^2.1.39",
    "@types/urijs": "^1.19.19",
    "jasmine-core": "~4.1.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.1",
    "prettier": "~2.6.2",
    "protractor": "^7.0.0",
    "ts-node": "~10.7.0",
    "tsickle": "^0.46.0",
    "tslint": "~6.1.3",
    "typescript": "~4.5.5"
  }
}
